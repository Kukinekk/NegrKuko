
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Kuko's Ware", "DarkTheme")
local Main = Window:NewTab("Main")
local Player = Window:NewTab("Player")
local Settings = Window:NewTab("Settings")
local MainSection = Main:NewSection("Lock")
local PlayerSection = Player:NewSection("Animation Changer")
local SettingsSection = Settings:NewSection("UI Closer")
SettingsSection:NewKeybind("Toggle UI", "Close UI with V", Enum.KeyCode.V, function()
	Library:ToggleUI()
end)
MainSection:NewButton("Start Locking Keybind (E)", "Starts The Aimlock", function()
   loadstring(game:HttpGet('https://pastebin.com/raw/xP0YwtAx'))()
end)
MainSection:NewTextBox("Predaction", "Set the Silent Aim Predaction", function(txt)
	print(txt)
end)
PlayerSection:NewButton("Good Animations", "Gives U Good Anims", function()
    print("Clicked")
      loadstring(game:HttpGet('https://pastebin.com/raw/DGsWJfCv'))()
      end)
